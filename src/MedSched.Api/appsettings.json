{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "Console"
      }
    ]
  },
  "ConnectionStrings": {
    "PostgresConnection": "Host=localhost;Port=5432;Database=MedSchedDb;Username=MedSchedUser;Password=MedSchedPass"
  },
  "AllowedHosts": "*",
  "DefaultPostgresPort": 5432,
  "NumberOfMeetingSuggestions": 4,
  "RENDER_POSTGRES_URL":"postgres://MedSchedUser:MedSchedPass@postgres:5432/MedSchedDb"
}
